<html>
<head>
    <style>
        table {
            width: 80%;
            margin: 20px auto;
        }
        th, td {
            border: 1px solid #ddd;
            padding: 10px;
            text-align: left;
        }
    </style>
</head>
<body>
<h1>Grades</h1>
<hr />
<a href="/">Home</a><br />
<hr />
<table>
    <tr>
        <th>Student</th>
        <th>Module</th>
        <th>Grade</th>
    </tr>
    <% students.forEach((student) => { %>
    <tr>
        <td><%= student.name %></td>
        <td><%= student.module %></td>
        <td><%= student.grade %></td>
    </tr>
    <% }) %>
</table>
</body>
</html>